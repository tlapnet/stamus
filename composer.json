{
  "name": "tlapnet/stamus",
  "description": "State Machine",
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/tlapnet/stamus",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "ninjify/qa": "^0.9.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Stamus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Tlapnet\\Stamus\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.1.x-dev"
    }
  }
}
